package com.tcs.user.model;

import java.util.List;

public class UserDTO {
	
	private int id;
	private String name;
	private String mobile;
	private String address;
	private List<Bill> bills;
	public UserDTO(int id, String name, String mobile, String address, List<Bill> bills) {
		super();
		this.id = id;
		this.name = name;
		this.mobile = mobile;
		this.address = address;
		this.bills = bills;
	}
	public UserDTO() {
		super();
		// TODO Auto-generated constructor stub
	}
	public UserDTO(String name, String mobile, String address, List<Bill> bills) {
		super();
		this.name = name;
		this.mobile = mobile;
		this.address = address;
		this.bills = bills;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getMobile() {
		return mobile;
	}
	public void setMobile(String mobile) {
		this.mobile = mobile;
	}
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}
	public List<Bill> getBills() {
		return bills;
	}
	public void setBills(List<Bill> bills) {
		this.bills = bills;
	}
	@Override
	public String toString() {
		return "UserDTO [id=" + id + ", name=" + name + ", mobile=" + mobile + ", address=" + address + ", bills="
				+ bills + "]";
	}
	
	

}
